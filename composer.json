{
  "name": "evelabs/oauth2-eveonline",
  "description": "EvE Online OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleg Krasavin",
      "email": "okwinza@gmail.com",
      "homepage": "https://github.com/okwinza"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "eveonline"
  ],
  "require": {
    "php": ">=5.5.0",
    "league/oauth2-client": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Evelabs\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Evelabs\\OAuth2\\Client\\Test\\": "test/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
